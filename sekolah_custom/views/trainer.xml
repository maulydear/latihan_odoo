<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<data>
		<record id="view_sekolah_custom_trainer_tree" model="ir.ui.view">
			<field name="name">sekolah.custom.trainer.tree</field>
			<field name="model">sekolah.custom.trainer</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="trainer">
					<field name="name"/>
				</tree>
			</field>
		</record>
		<record id="view_sekolah_custom_trainer_form" model="ir.ui.view">
			<field name="name">sekolah.custom.trainer.form</field>
			<field name="model">sekolah.custom.trainer</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">

				<form string="Trainer Form">
				  	<sheet>
							<label for="name" class="oe_edit_only" string="Trainer"></label>
							<h1>
								<field name="name"/>
							</h1>
				  	</sheet>
				</form>
			</field>
		</record>

	</data>
</odoo>