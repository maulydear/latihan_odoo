<?xml version="1.0" encoding="utf-8" ?>
<odoo>
	<data>

		<record id="view_sekolah_t_ujian_det_tree" model="ir.ui.view">
			<field name="name">sekolah.t_ujian_det.tree</field>
			<field name="model">sekolah.t_ujian_det</field>
			<field name="type">tree</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">
				<tree string="t_ujian_det">
					<field name="matapelajaran_id"/>
					<field name="nilai"/>
				</tree>
			</field>
		</record>

		<record id="view_sekolah_t_ujian_det_form" model="ir.ui.view">
			<field name="name">sekolah.t_ujian_det.form</field>
			<field name="model">sekolah.t_ujian_det</field>
			<field name="type">form</field>
			<field name="priority" eval="8"/>
			<field name="arch" type="xml">

				<form string="Ujian Detail Form">
				  <sheet>

					<group>
						<field name="matapelajaran_id"/>
						<field name="nilai"/>
					</group>

				  </sheet>
				</form>
			</field>
		</record>
	
	</data>
</odoo>